# Template - Ne pas committer avec de vrais secrets !
# Copier ce fichier vers secrets.yaml et remplir les valeurs.
apiVersion: v1
kind: Secret
metadata:
  name: achat-groupe-secrets
type: Opaque
stringData:
  QUARKUS_DATASOURCE_JDBC_URL: "jdbc:postgresql://postgres:5432/achatgroupe"
  QUARKUS_DATASOURCE_USERNAME: "changeme"
  QUARKUS_DATASOURCE_PASSWORD: "changeme"
  QUARKUS_OIDC_AUTH_SERVER_URL: "https://keycloak.example.com/realms/achatgroupe"
  QUARKUS_OIDC_CLIENT_ID: "achat-groupe"
  QUARKUS_OIDC_CREDENTIALS_SECRET: "changeme"
